#ifndef SRC_INCLUDE_DEVICE_SPI_W25Q256_H_
#define SRC_INCLUDE_DEVICE_SPI_W25Q256_H_


/*
  ** Include
*/
#include "xil_types.h"


/*
  ** Define
*/


/*
  ** Typedef
*/
typedef struct REG_STS {
	union {
		struct {
			/* Erase/Write In Progress . Read Only */
			u32 BUSY : 1;
			/* Write Enable Latch . Read Only */
			u32 WEL : 1;
			/* Block Protect Bit0 */
			u32 BP0 : 1;
			/* Block Protect Bit1 */
			u32 BP1 : 1;
			/* Block Protect Bit2 */
			u32 BP2 : 1;
			/* Block Protect Bit3 */
			u32 BP3 : 1;
			/* Top/Buttom Protect Bit */
			u32 TB : 1;
			/* Status Register Protect */
			u32 SRP : 1;
			/* Status Register Protect1 */
			u32 SRL : 1;
			/* Quad Enable */
			u32 QE : 1;
			/* Reserved */
			u32 R : 1;
			/* Security Register Lock Bit1 */
			u32 LB1 : 1;
			/* Security Register Lock Bit2 */
			u32 LB2 : 1;
		};
		u8 Data;
	};
}Reg_Sts_t;


#endif /* SRC_INCLUDE_DEVICE_SPI_W25Q256_H_ */


